<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui"> 
    <ribbon startFromScratch="false"> 
        <tabs>
            <tab id="CustomTab" label="QA Toolbar"> 
                <group id="Formatting" label="Formatting"> 
                    <button id="autoHeaderIngest"
                            imageMso="ControlPropertyWidthValue"
                            size="large"
                            label="Format Headers"
                            onAction="autoHeaderIngest"
                    />
                    <button id="SheetFixIngest"
                            imageMso="CssReports"
                            size="large"
                            label="Format Content"
                            onAction="SheetFixIngest"
                    />
                    <button id="autoHeaderFormatterIngest"
                            imageMso="CreateWorkflow"
                            size="large"
                            label="Format Header and Content"
                            onAction="autoHeaderFormatterIngest"
                    />
                    <separator id="separator1" />
                    <editBox id="gtxValue"
                             label="GTX-"
                             sizeString="0000000"
                             maxLength="4"
                             onChange="gtxValue_onChange"
                    />
                    <button id="autoHeaderUniquinizerIngest"
                            imageMso="ControlPropertyDefaultValue"
                            size="normal"
                            label="New UTIs"
                            onAction="autoHeaderUniquinizerIngest"
                    />
                </group>
                <group id="Shortcuts" label="Shortcuts">
                    <button id="scrollToUti"
                            imageMso="AddNewColumnMenu"
                            size="large"
                            label="Scroll To UTI"
                            onAction="findTradeID"
                    />
                    <button id="neSplit"
                            imageMso="AdpDiagramArrangeTables"
                            size="large"
                            label="New/Exit Split"
                            onAction="neSplit"
                    />
                    <separator id="separator2" />
                    <button id="textToCol"
                            imageMso="ConvertTextToTable"
                            size="large"
                            label="Delimit Text"
                            onAction="textToCol"
                    />
                    <checkBox id="TabCheckBox"
                              label="Tab"
                              onAction="boxChecked"
                    />
                    <checkBox id="SmcCheckBox"
                              label="Semicolon"
                              onAction="boxChecked"
                    />
                    <checkBox id="CmmCheckBox" 
                              label="Comma"
                              onAction="boxChecked"
                    />
                    <checkBox id="SpaCheckBox" 
                              label="Space"
                              onAction="boxChecked"
                    />
                    <editBox id="OthValue"
                             label="Other:"
                             sizeString="0"
                             maxLength="1"
                             onChange="OthValue_onChange"
                    />
                </group>
                <group id="reports" label="Reports">
                    <editBox id="batchValue"
                             label="Batch:"
                             sizeString="00000000"
                             maxLength="6"
                             onChange="batchValue"
                    />
                    <button id="pullRptsBtn"
                            imageMso="AccessReportMore"
                            size="normal"
                            label="Pull Reports"
                            onAction="pullReports"
                    />
                </group>
            </tab> 
        </tabs> 
    </ribbon> 
</customUI> 
